  <form action="/add" method="POST">
  
    <div class="form-group mb-4">
      <div class="col">
        <label for="Id">Id</label>
        <input type="number" class="form-control" id="Id" name="Id" value="" placeholder="Id" required>
      </div>
  
      <div class="col">
        <label for="fullName">Name</label>
        <input type="text" class="form-control" id="fullName" name="fullName" value="" placeholder=" Name" required>
      </div>
      <div class="col">
        <label for="Contact">Contact</label>
        <input type="text" class="form-control" id="Contact" name="Contact" value="" placeholder="Contact" required>
      </div>
      <div class="col">
        <label for="phoneNumber">Phone Number</label>
        <input type="number" class="form-control" id="phoneNumber" name="phoneNumber" value="" placeholder="Phone Number" required>
      </div>
      <div class="col">
          <label for="email">Email</label>
          <input type="text" class="form-control" id="email" name="email" value="" placeholder="Email" required>
        </div>
        <div class="col">
            <label for="webId">Web Id</label>
            <input type="text" class="form-control" id="webId" name="webId" value="" placeholder="Web Id" required>
        </div>
        <div class="col">
            <label for="amount">Amount</label>
            <input type="text" class="form-control" id="amount" name="amount" value="" placeholder="Amount" required>
        </div>
        <div class="col">
            <label for="exp">Date Of Exception</label>
            <input type="date" class="form-control" id="exp" name="exp" value="" placeholder="DD/MM/YYYY" required>
        </div>
        
        <div class="form-group mb-4">
            
            <button type="submit" class="btn btn-primary">Add User</button>
        </div>
    </div>
    </form>

